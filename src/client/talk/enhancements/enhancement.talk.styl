#EnhancementTalk
  position absolute
  left 350px
  width 350px
  height 270px
  padding 10px
  background-color grey
  border 5px black solid
  border-radius 1em
  z-index 100

#EnhancementTalkDialog
  position absolute
  font-size 1.1em

#EnhancementTalkDialogButton
  position absolute
  left 120px
  bottom 10px

#EnhancementTalkChoice
  position absolute
  font-size 1.1em

#EnhancementTalkChoiceYes
  position absolute
  bottom 10px
  left 10px

#EnhancementTalkChoiceNo
  position absolute
  bottom 10px
  right 10px

#EnhancementTalkAcknowledgement
  position absolute
  font-size 1.5em
  font-weight bold

#EnhancementTalkAcknowledgementButton
  position absolute
  font-size 1.5em
  font-weight bold
  bottom 10px
  left 130px
